> {%
    client.global.set("access_token", response.body.access_token);
%}


######### Document creation
POST {{PortfolioBackend_HostAddress}}/api/docugroup/documents
Content-Type: application/json
Authorization: Bearer {{access_token}}

{
  "Title": "WE BALL"
}

### add some components to the document
POST {{PortfolioBackend_HostAddress}}/api/docugroup/components
Content-Type: application/json
Authorization: Bearer {{access_token}}

{
  "LastPublishedContentJson": "WE RE HERE 222",
  "ComponentType": 1,
  "GroupDocumentId": "b829d1a1-2a5d-4c78-aa4e-71c01a86db89"
}

### add some paragraph components to the document
POST {{PortfolioBackend_HostAddress}}/api/docugroup/components
Content-Type: application/json
Authorization: Bearer {{access_token}}

{
  "LastPublishedContentJson": "This is not the end of the world, this is just the end of the chapter",
  "ComponentType": 2,
  "GroupDocumentId": "b829d1a1-2a5d-4c78-aa4e-71c01a86db89"
}

### publish the document
POST {{PortfolioBackend_HostAddress}}/api/docugroup/documents/publish
Content-Type: application/json
Authorization: Bearer {{access_token}}

{
  "Id": "b829d1a1-2a5d-4c78-aa4e-71c01a86db89"
}

### get the published document
GET {{PortfolioBackend_HostAddress}}/api/docugroup/documents/b829d1a1-2a5d-4c78-aa4e-71c01a86db89
Accept: application/json
Authorization: Bearer {{access_token}}

### get the Draft document
GET {{PortfolioBackend_HostAddress}}/api/docugroup/documents/b829d1a1-2a5d-4c78-aa4e-71c01a86db89/draft
Accept: application/json
Authorization: Bearer {{access_token}}